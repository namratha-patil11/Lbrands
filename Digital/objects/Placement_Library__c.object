    <validationRules>
        <fullName>Approver_Mandatory</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISPICKVAL( Offer_Type__c ,&quot;Offer&quot;),ISBLANK( Approver__c ))</errorConditionFormula>
        <errorDisplayField>Approver__c</errorDisplayField>
        <errorMessage>Approver is Mandatory..!!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Editorial_Name_Mandatory</fullName>
        <active>true</active>
        <description>Editorial name is mandatory when page type is Editorial</description>
        <errorConditionFormula>AND(
ISPICKVAL( Page_Types__c , &quot;Editorials&quot;),
ISBLANK(Editorial_Name__c)
)</errorConditionFormula>
        <errorDisplayField>Editorial_Name__c</errorDisplayField>
        <errorMessage>Editorial Name is Mandatory for Editorials, Please enter Editorial Name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_date_should_be_less_than_Start_date</fullName>
        <active>true</active>
        <errorConditionFormula>OR( End_Date__c &lt;  Start_Date__c , if(End_Date__c ==   Start_Date__c , if( End_Time_No__c  &lt;   Start_Time_No__c , true,false) ,false))</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>Start Date cannot be after End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Placement_Type_Mandatory</fullName>
        <active>true</active>
        <description>Placement_type__c is Mandatory when page type is Editorials</description>
        <errorConditionFormula>AND(
ISPICKVAL(Page_Types__c , &quot;Editorials&quot;),
ISPICKVAL(Placement_type__c,&quot;&quot;)
)</errorConditionFormula>
        <errorDisplayField>Placement_type__c</errorDisplayField>
        <errorMessage>This field is required for Editorials, Please complete the field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Shot_Number_Mandatory</fullName>
        <active>false</active>
        <errorConditionFormula>ISBLANK( Shot_Number__c )</errorConditionFormula>
        <errorDisplayField>Shot_Number__c</errorDisplayField>
        <errorMessage>Shot Number Required..!!</errorMessage>
    </validationRules>
