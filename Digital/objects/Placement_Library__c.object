    <fields>
        <fullName>End_Time_No__c</fullName>
        <externalId>false</externalId>
        <formula>IF(CONTAINS( TEXT(End_Time__c) , &#39;AM&#39;), 
IF(LEN(TEXT(End_Time__c))=5, 
IF(CONTAINS( TEXT(End_Time__c) , &#39;12&#39;),0,VALUE(LEFT(TEXT(End_Time__c),2))), 
VALUE(LEFT(TEXT(End_Time__c),1)) 
), 
IF(LEN(TEXT(End_Time__c))=5, 
VALUE(LEFT(TEXT(End_Time__c),2))+12, 
VALUE(LEFT(TEXT(End_Time__c),1))+12 
) 
) * 60 + VALUE(Text(End_Minute__c))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
